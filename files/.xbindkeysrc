# Volume
"amixer set Master 5+"
  Mod2 + XF86AudioRaiseVolume

"amixer set Master 5-"
  Mod2 + XF86AudioLowerVolume

"amixer sset Master toggle"
  XF86AudioMute



# Next, prev, play
"playerctl next"
  Mod2 + XF86AudioNext

"playerctl play-pause"
  Mod2 + XF86AudioPlay

"playerctl previous"
  Mod2 + XF86AudioPrev



# Keybrightness
"asus-kbd-backlight up"
  Mod2 + XF86KbdBrightnessUp

"asus-kbd-backlight down"
  Mod2 + XF86KbdBrightnessDown



# Screen brightness
"xbacklight -inc 10 -time 0 -steps 1"
  Mod4 + F6

"xbacklight -dec 10 -time 0 -steps 1"
  Mod4 + F5
